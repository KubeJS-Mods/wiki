{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"typeRoots": [
			"typings"
		],
		"baseUrl": ".",
		"paths": {
			"@java/*": [
				"typings/java/*"
			]
		},
		"lib": [
			"ES2015",
			"ES5"
		]
	}
}